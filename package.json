{
  "name": "parkings-svelte",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" vite build",
    "build-storybook": "storybook build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier src --write . --ignore-path .gitignore",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "eslint --fix",
    "storybook": "storybook dev -p 6006",
    "pre-commit": "npm run lint:fix && npm run format && tsc",
    "prepare": "git config core.hooksPath .git-hooks || echo 'Not in a git repo'",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@internationalized/date": "^3.7.0",
    "@lucide/svelte": "^0.482.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-svelte-csf": "^5.0.0",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/svelte": "^8.6.12",
    "@storybook/sveltekit": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/postcss": "^4.0.14",
    "@types/leaflet": "^1.9.17",
    "@types/leaflet.heat": "^0.2.4",
    "bits-ui": "^1.5.1",
    "clsx": "^2.1.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "formsnap": "^2.0.1",
    "globals": "^15.14.0",
    "lint-staged": "^15.4.1",
    "paneforge": "^1.0.0-next.5",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.6.12",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "sveltekit-superforms": "^2.23.1",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^5.4.11",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/svelte-query": "^5.76.0",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "sveaflet": "^0.1.4",
    "sveaflet-heat": "^0.0.4",
    "tailwindcss": "^4.1.4"
  }
}
